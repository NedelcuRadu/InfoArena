#include <iostream>
#include <fstream>
#include <math.h>
#define MAX 2000010
using namespace std;
int A[MAX];

void SieveOfErathostene() {
    for(int i = 1; i < MAX; i++)
        A[i] = 1;
    for(int i = 2; i < sqrt(MAX); i++)
        if(A[i])
            for(int j = i * i; j < MAX; j = j + i)
                A[j] = 0;

}

int main() {
    int n, k = 0;
    cin >> n;
    SieveOfErathostene();
    for(int i = 2; i <= n; i++)
        if(A[i])
            k++;
    cout << k;
    return 0;
}
